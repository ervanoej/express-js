mixin navbar()
  nav.navbar.navbar-expand.navbar-dark.bg-primary
    a(href="#",aria-label="Leads").navbar-brand.mr-0
    .mr-md-2
    .collapse.navbar-collapse
      ul.navbar-nav.mr-auto
        li.nav-item.active
          a.nav-link(href="#").span Home
        li.nav-item
          a.nav-link(href="/leads") Leads
